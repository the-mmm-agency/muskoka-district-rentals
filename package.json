{
  "name": "muskoka-district-rentals",
  "private": true,
  "description": "A booking pwa for Muskoka District Rentals built in GatsbyJS",
  "version": "0.0.1",
  "author": "Brett Mandler <brett@themmmagency.com>",
  "dependencies": {
    "dayjs": "^1.8.13",
    "gatsby": "^2.4.0",
    "gatsby-background-image": "^0.5.2",
    "gatsby-cli": "^2.5.12",
    "gatsby-image": "^2.0.41",
    "gatsby-source-filesystem": "^2.0.33",
    "gatsby-transformer-json": "^2.1.11",
    "gatsby-transformer-sharp": "^2.1.18",
    "intersection-observer": "^0.6.0",
    "polished": "^3.2.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-day-picker": "^7.3.0",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-star-ratings": "^2.3.0",
    "react-typography": "^0.16.19",
    "styled-components": "^4.2.0",
    "styled-components-breakpoint": "^2.1.1",
    "styled-theming": "^2.2.0",
    "styled-tools": "^1.7.1",
    "typescript": "^3.4.5",
    "typography": "^0.16.19",
    "use-onclickoutside": "^0.3.1"
  },
  "devDependencies": {
    "@percy/cypress": "^1.0.8",
    "babel-jest": "^24.7.1",
    "babel-plugin-polished": "^1.1.0",
    "babel-plugin-styled-components": "^1.10.0",
    "babel-preset-gatsby": "^0.1.11",
    "cypress": "^3.2.0",
    "cypress-testing-library": "^3.0.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-config-react-app": "^4.0.0",
    "eslint-formatter-pretty": "^2.1.1",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-cypress": "^2.2.1",
    "eslint-plugin-jest": "^22.5.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react-hooks": "^1.6.0",
    "gatsby-plugin-brotli": "^1.0.1",
    "gatsby-plugin-eslint": "^2.0.5",
    "gatsby-plugin-manifest": "^2.1.0",
    "gatsby-plugin-nprogress": "^2.0.10",
    "gatsby-plugin-offline": "^2.1.0",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-root-import": "^2.0.5",
    "gatsby-plugin-sharp": "^2.0.35",
    "gatsby-plugin-styled-components": "^3.0.7",
    "gatsby-plugin-typography": "^2.2.13",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.7.1",
    "jest-chain": "^1.1.2",
    "jest-dom": "^3.1.4",
    "jest-extended": "^0.11.1",
    "jest-styled-components": "^6.3.1",
    "jest-watch-typeahead": "^0.3.0",
    "prettier": "^1.17.0",
    "react-test-renderer": "^16.8.6",
    "source-map-support": "^0.5.12",
    "start-server-and-test": "^1.9.0",
    "stylelint": "^10.0.1",
    "stylelint-config-styled-components": "^0.1.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "now-build": "npm run build",
    "develop": "gatsby develop",
    "develop:ssl": "gatsby develop --ssl",
    "format": "prettier --write src/**/*.{js,jsx}",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "lint:styles": "stylelint src/**/*.js",
    "lint:styles:fix": "stylelint src/**/*.js --fix",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "jest",
    "test:watch": "jest --watch --verbose",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:e2e:dev": "start-server-and-test develop http://localhost:8000 cy:open",
    "test:e2e:run": "start-server-and-test develop http://localhost:8000 cy:run",
    "test:e2e:percy": "percy exec -- npm run test:e2e:run",
    "validate": "npm run lint && npm run lint:styles && npm run test && npm run test:e2e:percy"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brettm12345/muskoka-district-rentals"
  },
  "bugs": {
    "url": "https://github.com/brettm12345/muskoka-district-rentals/issues"
  },
  "babel": {
    "plugins": [
      "babel-plugin-polished"
    ],
    "presets": [
      [
        "babel-preset-gatsby",
        {
          "targets": {
            "browsers": [
              ">0.25%",
              "not dead"
            ]
          }
        }
      ]
    ]
  },
  "eslintConfig": {
    "globals": {
      "__PATH_PREFIX__": "true"
    },
    "extends": [
      "react-app",
      "prettier",
      "prettier/react"
    ],
    "plugins": [
      "prettier"
    ],
    "parserOptions": {
      "sourceType": "module",
      "ecmaFatures": {
        "jsx": true
      }
    },
    "env": {
      "es6": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    }
  },
  "prettier": {
    "endOfLine": "lf",
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5"
  },
  "stylelint": {
    "extends": "stylelint-config-styled-components",
    "rules": {
      "at-rule-empty-line-before": null
    }
  }
}
